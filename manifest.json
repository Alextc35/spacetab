{
  "manifest_version": 3,
  "name": "Spacetab",
  "version": "0.7.0",
  "description": "Nueva pesta√±a con favoritos editables",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "web_accesible_resources": [
    {
      "resources": ["js/*.js", "js/core/*.js", "js/ui/*.js", "css/*.css"],
      "matches": ["<all_urls>"]
    }
  ]
}